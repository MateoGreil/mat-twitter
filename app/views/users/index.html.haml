%h1 Users
%table
    %tr
        %th First Name
        %th Last Name
        %th Email
        %th Login
        
    - @users.each do |user|
        %tr
            %td= user.first_name
            %td= user.last_name
            %td= user.email
            %td= user.login
            %td= link_to 'Show', user_path(user)